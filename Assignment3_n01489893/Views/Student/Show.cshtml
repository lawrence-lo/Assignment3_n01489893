@model Assignment3_n01489893.Models.Student
@{
    ViewBag.Title = "Student Details";
}
<h1>Student Details</h1>
<a class="btn btn-default" role="button" href="/Student/List/">Back To List</a>

<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">@Model.StudentFname @Model.StudentLname</h3>
    </div>
    <div class="panel-body">
        <p>Student Number: @Model.StudentNumber</p>
        <p>Enrol Date: @Model.StudentEnrolDate.ToString("MMM dd, yyyy")</p>
    </div>
</div>